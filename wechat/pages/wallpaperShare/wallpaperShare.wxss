page {
  width: 100%;
  height: 100%;
}

.canvas-warp {
  background: #fff;
  position: relative;
  height: 100%;
}

.canvas {
  margin: 0 auto;
  box-shadow: 0rpx 0rpx 20rpx rgba(0, 0, 0, 0.3);
}

.menu-item {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}

.switch-warp {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 100rpx;
  font-size: 28rpx;
  color: #999;
}

.switch-warp switch {
  color: #5756c0;
  margin-right: 20rpx;
}

.btn-warp {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #666;
  padding: 40rpx 80rpx;
}
